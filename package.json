{
  "name": "airwaves",
  "private": true,
  "description": "A website for the Unlocking the Airwaves website",
  "version": "0.0.1",
  "author": "Ed Summers <ehs@pobox.com>",
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "elasticlunr": "^0.9.5",
    "gatsby": "^2.19.19",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-flexsearch": "^1.0.3",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-json": "^2.2.15",
    "mirador": "^3.0.0-alpha.16",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-highlight-words": "^0.16.0",
    "react-icons": "^3.8.0",
    "react-responsive-carousel": "^3.1.51",
    "react-slick": "^0.25.2",
    "rss": "^1.2.2",
    "sax": "^1.2.4",
    "slick-carousel": "^1.8.1",
    "webvtt-player": "0.0.10"
  },
  "devDependencies": {
    "airtable": "^0.7.2",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.34",
    "glob": "^7.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.13.0"
  },
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby clean && gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "fetch-authorities": "node --harmony src/commands/fetch-authorities.js",
    "fetch-episodes": "node --harmony src/commands/fetch-episodes.js",
    "fetch-docs": "node --harmony src/commands/fetch-docs.js",
    "fetch-data": "npm run fetch-authorities && npm run fetch-docs && npm run fetch-episodes",
    "index": "node --harmony src/commands/index.js",
    "audit": "node --harmony src/commands/audit.js",
    "publish": "npm run build && rsync -ar public/ ubuntu@wordpress.mith.us:/var/www/html/static/archive.mith.umd.edu/airwaves"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umd-mith/airwaves"
  },
  "bugs": {
    "url": "https://github.com/umd-mith/airwaves/issues"
  }
